\he rare
\de voiture, véhicule à moteur
\dn car, motor vehicle
\lf Empr.
\lv tarak
\se n-age voyopyot
\ps n
\lt objet pour ronger (le bois)
\de scie; tronçonneuse
\dn saw; chainsaw
\lf Empr.
\lv jenso
\se n-age leplep hohole
\ps n
\lt objet pour prendre les paroles
\de magnétophone
\dn recorder
\lf Empr.
\lv tēprēkota
\se n-age leplep yoy
\ps n
\va n-age tēytēy yoy
\lt objet pour prendre / retenir les nouvelles
\de répondeur téléphonique
\dn answering machine
\rf AP9-48
\se n-age bo-totgal
\ps n
